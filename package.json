{
    "private": true,
    "name": "remix-app-template",
    "description": "",
    "license": "",
    "scripts": {
        "build": "npm run build:css && remix build",
        "build:css": "tailwindcss -i ./css/tailwind.css -o ./app/tailwind.css --minify",
        "dev": "concurrently \"npm run dev:css\" \"cross-env NODE_ENV=development netlify dev\"",
        "dev:css": "tailwindcss -i ./css/tailwind.css -o ./app/tailwind.css --watch",
        "start": "remix-serve build"
    },
    "dependencies": {
        "@imgix/js-core": "^3.8.0",
        "@netlify/functions": "^1.4.0",
        "@remix-run/netlify": "^1.11.1",
        "@remix-run/node": "^1.11.1",
        "@remix-run/react": "^1.11.1",
        "@sendgrid/mail": "^7.7.0",
        "akismet-api": "^5.3.0",
        "classnames": "^2.3.2",
        "dotenv": "^16.0.3",
        "graphql": "^16.6.0",
        "graphql-request": "^5.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-syntax-highlighter": "^15.5.0"
    },
    "devDependencies": {
        "@remix-run/dev": "^1.11.1",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@types/react-syntax-highlighter": "^15.5.6",
        "@viget/tailwindcss-plugins": "^3.0.0",
        "concurrently": "^7.6.0",
        "cross-env": "^7.0.3",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "prettier": "2.8.3",
        "tailwindcss": "^3.2.4",
        "typescript": "^4.9.4"
    },
    "engines": {
        "node": ">=14"
    },
    "sideEffects": false
}
